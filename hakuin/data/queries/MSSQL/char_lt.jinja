{% set single = true %}
{% extends "_base_schema.jinja" %}


{% block select %}
SELECT  CASE WHEN
            {{ column | sql_to_varchar | sql_len }} != {{ ctx.buffer | length }}
            AND
            {{ column | sql_to_varchar | sql_char_at(ctx.buffer | length) | sql_to_unicode }} < {{ n }}
        THEN 1 ELSE 0 END
{% endblock %}
